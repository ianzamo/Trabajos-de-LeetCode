Ejercicio 1.1: 

mensaje1 = input("Inserta un mensaje: ")
mensaje2 = input("Inserta otro mensaje: ")

if(mensaje1 == mensaje2):
    print("Los mensajes son iguales.")
else:
    print("Los mensajes son distintos.")

#Ejercicio 1.2: 

nombre_alumno = input("Inserte el nombre del alumno: ")

if not nombre_alumno.strip():
    print("El nombre es incorrecto.")
else:
    print("Se ha ingresado correctamente.")

#Ejercicio 1.3: 

edad = int(input("Digite la edad: "))

if edad >= 18:
    print("Es mayor de edad.")
else:
    print("Es menor de edad.")

Ejercicio 2.1:

a = 0

while a < 10:
    print(a)
    a+=1

Ejercicio 2.2:

while True: 

    nombre_usuario = input("Ingrese su nombre de usuario: ")

    if not nombre_usuario.strip():
        print("No puede estar vacio, intente nuevamente: ")
        continue
    break
print("Hasta luego", nombre_usuario)

Ejercicio 3:

nombres = ["Susana", "Alejandro", "Roberto"]

nombres.insert(2, "Paula")
nombres.append("Silvina")

print(nombres)

Ejercicio 4:

nombres = ["Agustina", "Marisa", "Juan", "Osvaldo"]

for i in nombres:
    print(i)

Ejercicio 5:

matriz = [[3.3, 6.1, 4.0], [4.9, 5.7, 6.4]]

while True: 

    fila = int(input("Ingrese la fila a buscar: "))
    columna = int(input("Ingrese la columna a buscar: "))

    if (fila >= 0 and fila <=1)  and (columna >= 0 and columna <=2):

        print(matriz[fila][columna])   
    else:   
        print("El numero de fila y columna no existe, ingrese otro.")
        break
        
Ejercicio 6.1: 

while True: 
    edad = int(input("Ingrese la edad: "))

    if 0 < edad < 18:
        print("La persona es menor de edad.")
    elif 18 <= edad < 65:
        print("La persona es mayor con edad laboral.")
    elif edad >=65:
        print("La persona es jubilada.")
    else: 
        print("El numero ingresado es incorrecto.")
        break

Ejercicio 6.2:

m = [ [10,50,5], [20,30,70], [15,45,80] ]

for fila in m:
    for elemento in fila:
        print(elemento)

Ejercicio 7: 

a = "Cancún 7 noches + aéreos: u$s 1200 por persona."
b = "Miami 8 noches + aéreos + alquiler de auto: u$s 1500 por persona."
c = "Bariloche 10 noches + aéreos + excursiones: u$s 1300 por persona."
d = "Río de Janeiro 10 noches + aéreos + excursiones: u$s 1400 por persona."

while True:
    eleccion_paquete = str(input("Ingresa el paquete deseado: ")).lower()

    if eleccion_paquete == 'a':
        print(a)
        break
    elif eleccion_paquete == 'b':
        print(b)
        break
    elif eleccion_paquete == 'c':
        print(c)
        break
    elif eleccion_paquete == 'd':
        print(d)
        break
    else: 
        print("El paquete seleccionado no existe.")
        break
